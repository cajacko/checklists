{
  "name": "checklists",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/cajacko/checklists.git",
  "author": "Charlie Jackson <charlie@charliejackson.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn build:tsc && concurrently \"yarn start:server\" \"yarn start:tsc\"",
    "start:server": "NODE_ENV=development nodemon dist/index.js",
    "start:tsc": "tsc --watch",
    "build:tsc": "tsc",
    "dev:build": "yarn data:clean && docker build -t charliejackson/node-web-app .",
    "dev:remove": "docker rm charlie-jackson-personal-server || true",
    "dev:stop": "docker stop charlie-jackson-personal-server || true",
    "dev:run": "yarn dev:stop && yarn dev:remove && docker run -e \"NODE_ENV=staging\" --name charlie-jackson-personal-server -p 49160:8080 -d charliejackson/node-web-app",
    "dev:build-run": "yarn dev:build && yarn dev:run",
    "dev:heroku-logs": "heroku logs --tail -a charlie-jackson-server",
    "data:clean": "cd data && git clean -fxd",
    "sync": "cp -r server/shared web/src",
    "kill-port": "kill -9 $(lsof -ti:8080)",
    "test": "jest --testPathIgnorePatterns web dist --watch"
  },
  "dependencies": {
    "@types/cors": "2.8.8",
    "@types/cron": "1.7.2",
    "@types/express": "4.17.8",
    "@types/fs-extra": "9.0.4",
    "@types/glob": "7.1.3",
    "@types/node-fetch": "2.5.7",
    "@types/uuid": "8.3.0",
    "bottleneck": "2.19.5",
    "cors": "2.8.5",
    "cron-parser": "2.16.3",
    "cron": "1.8.2",
    "csv-parse": "4.14.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "firebase-admin": "9.3.0",
    "fs-extra": "9.0.1",
    "glob": "7.1.6",
    "node-fetch": "2.6.1",
    "ts-jest": "26.4.4",
    "typescript": "4.0.5",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.16",
    "concurrently": "5.3.0",
    "jest": "26.6.3",
    "nodemon": "2.0.6"
  }
}
